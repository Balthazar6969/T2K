# Master package to specify highland2 package versions and the psyche released version

# psyche packages
use nd280Psyche                       v3r25 

# base highland2 packages
use highlandCore                      v2r21   highland2
use oaAnalysisReader                  v2r9    highland2
use highlandEventModel                v2r19   highland2
use highlandTools                     v2r17p1 highland2
use highlandUtils                     v2r19   highland2
use highlandIO                        v2r21   highland2
use highlandCorrections               v2r7    highland2 
use highlandSystematics               v2r3    highland2
use baseAnalysis                      v2r19   highland2 

# tracker analyses
use baseTrackerAnalysis               v2r11   highland2 
use numuCCAnalysis                    v2r13   highland2
use numuCCMultiPiAnalysis             v2r7    highland2
use antiNumuCCAnalysis                v2r3    highland2
use numuCCMultiTargetAnalysis         v2r11   highland2
use numuBkgInAntiNuModeAnalysis       v2r5    highland2
use nueCCAnalysis                     v2r17   highland2
use antiNueCCAnalysis                 v2r9    highland2
use gammaAnalysis                     v2r11   highland2 

# P0D-tracker analyses
use p0dNumuCCAnalysis                 v2r13   highland2
use p0dNumuCCQEAnalysis               v2r11p1 highland2

# P0D-only analyses
use baseP0DAnalysis                   v2r5    highland2

# the tutorial analysis package
use tutorialAnalysis                  v2r15   highland2

# the validation package (version used to validate this nd280Highland2 version)
use highlandValidation                v2r15   highland2

# highland documentation
use highlandDoc                       v2r1    highland2
use highlandND280Doc                  v2r1    highland2


